{
  "name": "Magic Card Zoom",
  "version": "3.2.3",
  "manifest_version": 3,
  "description": "Identify Magic: The Gatheringâ„¢ cards in videos and streams",
  "icons": {
    "128": "icon128.png"
  },
  "background": {
    "service_worker": "background.bundle.js"
  },
  "permissions": [
    "declarativeContent",
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_icon": "icon128.png",
    "default_title": "MagicCardZoom",
    "default_popup": "popup.html"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
